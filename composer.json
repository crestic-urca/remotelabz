{
    "name": "crestic-urca/remotelabz",
    "description": "Education software to provide VMs and networks as-a-service.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "crestic-urca/network-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "friendsofsymfony/rest-bundle": "dev-master",
        "gesdinet/jwt-refresh-token-bundle": "^0.9",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^3.1",
        "knplabs/knp-markdown-bundle": "^1.7",
        "lexik/jwt-authentication-bundle": "^2.6",
        "limenius/react-bundle": "^5.0",
        "myclabs/deep-copy": "^1.9",
        "s1lentium/iptools": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.0.*",
        "symfony/console": "^5.0",
        "symfony/debug-bundle": "5.0.*",
        "symfony/debug-pack": "^1.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/messenger": "5.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer": "5.0.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "5.0.*",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/web-link": "^5.0",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.15",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "php -r \"file_exists('.env') || copy('.env.dist', '.env');\""
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "php -r \"file_exists('.env') || copy('.env.dist', '.env');\""
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/crestic-urca/network-bundle.git"
        }
    ]
}
